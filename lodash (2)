import _ from "lodash";

// 3. find / findIndex
// 4. remove

const users = [
  { userId: "1", name: "giro" },
  { userId: "2", name: "hwiy" },
  { userId: "3", name: "amy" },
  { userId: "4", name: "tom" },
];

const foundUser = _.find(users, { name: "giro" });
console.log(foundUser);

const foundUserIndex = _.findIndex(users, { name: "giro" });
console.log(foundUserIndex);

_.remove(users, { name: "hwiy" });
console.log(users);
